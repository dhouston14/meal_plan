<h1>Add New Food</h1>

<%= form_for @food do |f| %>
  <h2><%= f.label :food %></h2>
      <%= f.text_field :food_type %>

  <h2><%= f.label :calories %></h2>
      <%= f.text_field :calories %>

  <h2><%= f.label :image %></h2>
      <%= f.text_field :img_url%>

  <h2><%= f.label :describe %></h2>
      <%= f.text_field :describe %>

  <h3><%= f.submit %></h3>

  <% end %>

